		<rdfs:range rdf:resource="&roimport;BFO_0000015"/>
		<iof-av:adaptedFrom rdf:datatype="&xsd;anyURI">http://www.ontologyrepository.com/CommonCoreOntologies/Mid/ExtendedRelationOntology</iof-av:adaptedFrom>
		<iof-av:elucidation xml:lang="en">x is output of y iff x is an instance of Continuant and y is an instance of Process, such that the presence of x at the end of y is a necessary condition for the completion of y.</iof-av:elucidation>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&iof-core;isSpecifiedOutputOf">
