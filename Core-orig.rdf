        <rdfs:domain rdf:resource="https://www.industrialontologies.org/ontology/core/Core/RequirementSpecification"/>
        <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000001"/>
        <rdfs:label>satisfied by</rdfs:label>
