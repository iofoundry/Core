                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://purl.obolibrary.org/obo/BFO_0000040"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000196"/>
                                <owl:someValuesFrom rdf:resource="https://www.industrialontologies.org/ontology/core/Core/SupplierRole"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000057"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/core/Core/Product"/>
                            <rdf:Description rdf:about="https://www.industrialontologies.org/ontology/core/Core/ServiceAgreement"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>act of selling</rdfs:label>
        <skos:example>Good Year sells tires to GM, Boeing sells 737 and 10 Service Agreement to United Airlines.</skos:example>
        <iof-av:adaptedFrom>Common Core Ontology: Act of Buying</iof-av:adaptedFrom>
        <iof-av:elucidation>a business process wherein product or service agreement is offered by an agent (seller) for another agent (buyer) to acquire ownership via a financial instrument</iof-av:elucidation>
    </owl:Class>
    


